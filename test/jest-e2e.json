{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@requestify/requestify/(.*)": "<rootDir>/../libs/requestify/src/$1",
    "@requestify/requestify": "<rootDir>/../libs/requestify/src",
    "@nestlog/nestlog/(.*)": "<rootDir>/../libs/nestlog/src/$1",
    "@nestlog/nestlog": "<rootDir>/../libs/nestlog/src"
  }
}